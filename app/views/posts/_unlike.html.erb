<% if !current_user?(post.user) %>
  <%= form_for(current_user.favorite_relationships.find_by(post_id: post.id),
             html: { method: :delete }, remote: true) do |f| %>
    <%= button_tag( class: "btn btn-default") do %>
      <%= content_tag :span, post.liked_by.count, class: "glyphicon glyphicon-heart" %>
    <% end %>
  <% end %>
<% end %>
